              TenantAdminUrl="https://chrisobriensp-admin.sharepoint.com"
              SharePointOnPremises ="false"
              RootTemplate="STS#0"
              RootWebOnly="false"
              StorageMaximumLevel="110"
              StorageWarningLevel="0"
              UserCodeMaximumLevel="100"
              UserCodeWarningLevel="0"
              ProvisioningTemplateContainer="Resources/SiteTemplates/ProvisioningTemplate/"
              ProvisioningTemplate="TeamSiteTemplate.xml"
              Enabled="true"/>
    <Template Title="Content and Code client site"
              Description="Team sites should be used for team collaboration and offer basic document and project management features.  Team sites also offer a default team notebook and site feed"
              ImageUrl ="../images/template-icon.png"
              HostPath="https://chrisobriensp.sharepoint.com/sites/"
